    {"name": "num_lines_unit", "type": "float", "populate": "populate_num_lines_unit"},
    {"name": "num_shapes_unit", "type": "float", "populate": "populate_num_shapes_unit"},
    {"name": "width_plus_height", "type": "float", "populate": "populate_width_height"}
